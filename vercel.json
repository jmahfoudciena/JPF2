{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "GOOGLE_API_KEY": "@google_api_key",
    "GOOGLE_CX": "@google_cx"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}

